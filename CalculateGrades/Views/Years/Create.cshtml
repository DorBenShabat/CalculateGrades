@model Years

<form method="post">
<html dir="rtl">
    <div class="border p-3 mt-4">
        <div class="row pb-2">
            <h2>הוספת שנה שנים </h2>
        </div>
        <div class="mb-3 row p-1">
            <label asp-for="Year" style="font-size: x-large" class="p-0"></label>
            <input asp-for="Year" class="form-control" />
        </div>
        <div class="row">
            <div class="col-6 col-md-3">
                <button type="submit" class="btn btn-primary form-control">הוסף</button> 
            </div>
        </div>
    </div>

</html>
</form>